#import "Basic";
#import "String";

main :: () {
    part("bvwbjplbgvbhsrlpgdmjqwftvncz");
    part("nppdvjthqldpwncqszvftbrmjlhg");
    part("nznrnfrfntjfmvfwmzdfjlvtqnbhcprsg");
    part("zcfzfwzzqfrljwzlrfnpqdbhtmscgvjw");
    part(test);

    part("mjqjpqmgbljsphdztnvjfqwrcgsmlb", N=14);
    part("bvwbjplbgvbhsrlpgdmjqwftvncz", N=14);
    part("nppdvjthqldpwncqszvftbrmjlhg", N=14);
    part("nznrnfrfntjfmvfwmzdfjlvtqnbhcprsg", N=14);
    part("zcfzfwzzqfrljwzlrfnpqdbhtmscgvjw", N=14);
    part(test, N=14);
}

part :: (input : string, $N: int = 4) {
    four : [N]u8;
    for i: 0..input.count-1 {
        four[i % N] = input[i];

        if i < N continue;

        // * could optimize, by only comparing the new entry to the other ones, 
        // * since they already got checked for each other.
        for a: 0..N-1 for b: a+1..N-1
            if four[a] == four[b] then continue i;

        print("index = %\n", i+1);
        break;
    }
}

test :: #string DONE

DONE